{
  "providers": {
    "openrouter": {
      "apiKey": "sk-or-v1-YOUR_OPENROUTER_KEY",
      "apiBase": "https://openrouter.ai/api/v1"
    },
    "openai": {
      "apiKey": "sk-YOUR_OPENAI_KEY"
    },
    "anthropic": {
      "apiKey": "sk-ant-YOUR_ANTHROPIC_KEY"
    }
  },
  "agents": {
    "defaults": {
      "workspace": "~/.crabbybot/workspace",
      "model": "anthropic/claude-sonnet-4-5",
      "maxTokens": 8192,
      "temperature": 0.7,
      "maxToolIterations": 20
    }
  },
  "tools": {
    "restrictToWorkspace": false,
    "webSearch": {
      "apiKey": "YOUR_SERP_OR_TAVILY_KEY",
      "maxResults": 5
    },
    "exec": {
      "timeoutSeconds": 30,
      "allowedCommands": []
    }
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "token": "YOUR_TELEGRAM_BOT_TOKEN",
      "allowFrom": []
    },
    "discord": {
      "enabled": false,
      "token": "YOUR_DISCORD_BOT_TOKEN",
      "allowFrom": []
    }
  },
  "gateway": {
    "host": "0.0.0.0",
    "port": 18790
  }
}
